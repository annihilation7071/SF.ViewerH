{
  "manifest_version": 3,
  "name": "SF.ViewerH.Downloader.Lite",
  "version": "1.1",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "http://127.0.0.1:1707/*"
  ],
  "action": {
    "default_title": "Send URL to Server"
  }
}