{
  "manifest_version": 3,
  "name": "Send Current URL",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "http://127.0.0.1:5000/*"
  ],
  "action": {
    "default_title": "Send URL to Server"
  }
}